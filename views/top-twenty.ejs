<% include components/head.ejs %>
<a class="btn --primary" href="/add-bands">Add Bands</a>
<% if(data.top_20.length > 0){%>
  <ul class="top-twenty-list">
    <% for(let i = 0; i < data.top_20.length; i++){ %>
      <li class="band --item"><%= data.top_20[i] _%>
      <button data-name="<%data.top_20[i]%>" data-method="delete">Delete</button>
      </li>
      <% if(i >= 20){
        break;
      }%>
    <% } %>
  </ul>
<% } %>

